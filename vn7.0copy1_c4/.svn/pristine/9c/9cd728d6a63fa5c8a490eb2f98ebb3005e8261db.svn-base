##############################################################################
## Extract and build jobs
###############################################################################
    [[METO_EX1A_CCE_EXTRACT]]
        inherit = None, EXTRACT, LINUX
        [[[environment]]]
            JULES_PLATFORM = meto-ex1a-cce
            JULES_REMOTE = remote

    [[METO_EX1A_CCE_BUILD]]
        inherit = None, METO_EX1A_CCE_ENV, METO_EX1A_QUEUE, BUILD
        [[[directives]]]
            -l ncpus = 8
            -l mem = 4GB
        [[[environment]]]
            ROSE_TASK_N_JOBS = 8
            JULES_PLATFORM = meto-ex1a-cce

# Debug build for Loobos jobs
#   Extract on Linux
    [[fcm_make_meto_ex1a_cce_debug]]
        inherit = METO_EX1A_CCE_EXTRACT
        [[[environment]]]
            JULES_BUILD = debug
            JULES_OMP = noomp
#   Build on EX1A
    [[fcm_make2_meto_ex1a_cce_debug]]
        inherit = METO_EX1A_CCE_BUILD
        [[[environment]]]
            JULES_BUILD = debug
            JULES_OMP = noomp

# Normal build without OpenMP
#   Extract on Linux
    [[fcm_make_meto_ex1a_cce_noomp]]
        inherit = METO_EX1A_CCE_EXTRACT
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP = noomp
#   Build on EX1A
    [[fcm_make2_meto_ex1a_cce_noomp]]
        inherit = METO_EX1A_CCE_BUILD
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP = noomp

# Normal build with OpenMP
#   Extract on Linux
    [[fcm_make_meto_ex1a_cce_omp]]
        inherit = METO_EX1A_CCE_EXTRACT
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP = omp
#   Build on EX1A
    [[fcm_make2_meto_ex1a_cce_omp]]
        inherit = METO_EX1A_CCE_BUILD
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP = omp

