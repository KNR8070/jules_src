###############################################################################
## Build jobs
###############################################################################
# Override SCRIPTS family.
# (normally found in runtime-common.rc, Done to enforce loading of
# Python 3 on JASMIN which is required by UMDP3 checker)
    [[SCRIPTS]]
            env-script = "module load jaspy/3.7/r20210320"

{% if REMOTE_JASMIN  %}
    [[REMOTE]]
        submission polling intervals = PT1M
        execution polling intervals = PT1M
        [[[remote]]]
            owner = {{ JASMIN_USER_ID }}
{% else %}
     [[REMOTE]]
{% endif %}


    [[JASMIN_LOTUS]]
        inherit = REMOTE
        env-script = """
                eval $(rose task-env)
                export PATH=/apps/jasmin/metomi/bin:$PATH
                module load intel/19.0.0
                module load contrib/gnu/gcc/7.3.0
                module load eb/OpenMPI/intel/3.1.1
                module list 2>&1
                export NETCDF_FORTRAN_ROOT=/gws/nopw/j04/jules/admin/netcdf/local_nc_par/3.1.1/intel.19.0.0/
                export NETCDF_ROOT=/gws/nopw/j04/jules/admin/netcdf/local_nc_par/3.1.1/intel.19.0.0/
                export HDF5_LIBDIR=/gws/nopw/j04/jules/admin/netcdf/local_nc_par/3.1.1/intel.19.0.0/lib
                export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$HDF5_LIBDIR
                env | grep LD_LIBRARY_PATH
                """

        [[[job]]]
            batch system = slurm
        [[[directives]]]
            --partition = par-multi
            --constraint="skylake348G"
        [[[environment]]]
            ROSE_LAUNCHER = mpirun
            NETCDF_FORTRAN_ROOT=/gws/nopw/j04/jules/admin/netcdf/local_nc_par/3.1.1/intel.19.0.0/
            NETCDF_ROOT=/gws/nopw/j04/jules/admin/netcdf/local_nc_par/3.1.1/intel.19.0.0/

        {% if JASMIN_JULES_BASE_DIR is defined %}
            JASMIN_JULES_BASE_DIR = {{JASMIN_JULES_BASE_DIR}}
        {% else %}
            JASMIN_JULES_BASE_DIR = "/gws/nopw/j04/jules"
        {% endif %}
            JASMIN_ROSE_STEM_DATA_DIR = $JASMIN_JULES_BASE_DIR/admin

    [[JASMIN_FCM_MAKE]]
        inherit = None, JASMIN_LOTUS, EXTRACT
        [[[environment]]]
            JULES_PLATFORM = jasmin-lotus-intel
            {% if REMOTE_JASMIN %}
                JULES_SOURCE = {{ REMOTE_JULES_SOURCE }}
            {% else %}
                JULES_SOURCE = {{ SOURCE_JULES }}
            {% endif %}
        [[[job]]]
            execution time limit = PT20M
        [[[directives]]]
            --ntasks = 1

    [[fcm_make_debug]]
        inherit = None, JASMIN_FCM_MAKE
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP   = noomp

    [[fcm_make_mpi]]
        inherit = None, JASMIN_FCM_MAKE
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP   = noomp

    [[fcm_make_mpi_rivers-only]]
        inherit = None, JASMIN_FCM_MAKE, EXTRACT_RIVER
        [[[environment]]]
            JULES_BUILD = normal
            JULES_OMP   = noomp

