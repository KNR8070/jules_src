    [[NCI_GFORTRAN_BUILD]]
        inherit = BUILD, NCI
        pre-script = """
            module use ~access/modules
            module load gcc/11.1.0
            module load openmpi/4.1.4
            module unload netcdf
            module load netcdf/4.8.0p
            module load fcm
        """
        [[[environment]]]
            ROSE_TASK_N_JOBS = 4
        [[[directives]]]
            -l ncpus = 4
            -l mem = 8GB
            -l jobfs = 2GB
            -q = normal

###############################################################################
## Build jobs
###############################################################################
# 
    [[fcm_make_nci_gfortran_debug]]
        inherit = NCI_EXTRACT, NCI_GFORTRAN_BUILD
        [[[environment]]]
            JULES_PLATFORM = nci-gfortran
            JULES_BUILD = debug
            JULES_OMP = noomp

    [[fcm_make_nci_gfortran_omp]]
        inherit = NCI_EXTRACT, NCI_GFORTRAN_BUILD
        [[[environment]]]
            JULES_PLATFORM = nci-gfortran
            JULES_BUILD = normal
            JULES_OMP = omp

